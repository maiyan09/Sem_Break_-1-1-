#include <iostream>
#include <set>
#include <string>
using namespace std;

int main()
{
    string arr;
    cin >> arr;
    
    set <char> res;
    for (char i : arr){
        res.insert(i);
    }
    
    if(res.size() % 2 == 0){
        cout << "CHAT WITH HER!" << endl;
    }
    else{
        cout << "IGNORE HIM!" << endl;
    }

    return 0;
}
